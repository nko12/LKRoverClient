<Window x:Name="windowMain" x:Class="LKRoverApp.WindowMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LKRoverApp"
        mc:Ignorable="d"
        Title="Lunar Knights Rover Controller" Height="350" Width="525">
    <Grid>
        <Button x:Name="buttonUCF" Content="Show UCF" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="buttonUCF_Click"/>
        <Button x:Name="buttonSpace" Content="Show Space" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="75" Click="buttonSpace_Click"/>
        <Button x:Name="buttonBackground" Content="Background" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Width="75" Click="buttonBackground_Click"/>
        <TextBox x:Name="textBoxName" HorizontalAlignment="Left" Height="23" Margin="175,7,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonShowName" Content="Display Name" HorizontalAlignment="Left" Margin="300,10,0,0" VerticalAlignment="Top" Width="85" Click="buttonShowName_Click"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="185,44,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>

    </Grid>
</Window>
